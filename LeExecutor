-- Scripts

local function YGCEOW_fake_script() -- Clear.LocalScript 
	local script = Instance.new('LocalScript', Clear)

	Clear.MouseButton1Click:Connect(function()
		TextBox.Text = ""
	end)
end
coroutine.wrap(YGCEOW_fake_script)()
local function MWULNW_fake_script() -- Execute.LocalScript 
	local script = Instance.new('LocalScript', Execute)

	Execute.MouseButton1Click:Connect(function()
		--Execute stuff here
		
	end)
end
coroutine.wrap(MWULNW_fake_script)()
local function XKPYO_fake_script() -- WIP.LocalScript 
	local script = Instance.new('LocalScript', WIP)

	--[[Inject.MouseButton1Click:Connect(function()
		--Inject stuff here
		
	end)]]--
end
coroutine.wrap(XKPYO_fake_script)()
local function HZSLU_fake_script() -- RJ.LocalScript 
	local script = Instance.new('LocalScript', RJ)

	RJ.MouseButton1Click:Connect(function()
		game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
	end)
end
coroutine.wrap(HZSLU_fake_script)()
local function MVIBTEH_fake_script() -- RE.LocalScript 
	local script = Instance.new('LocalScript', RE)

	RE.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.Humanoid.Health = 0
	end)
end
coroutine.wrap(MVIBTEH_fake_script)()
local function TRSNFUZ_fake_script() -- OpenClose.LocalScript 
	local script = Instance.new('LocalScript', OpenClose)

	
	OpenClose.MouseEnter:Connect(function()
		--[[local TweenService = game:GetService("TweenService")
		local tween = TweenService:Create(
			script.Parent,
			TweenInfo.new(1),
			{
				Rotation = 360
			}
		)
		tween:Play()]]--
	end)
	
	OpenClose.MouseLeave:Connect(function()
		--[[local TweenService = game:GetService("TweenService")
		local tween = TweenService:Create(
			OpenClose,
			TweenInfo.new(1),
			{
				Rotation = 0
			}
		)
		tween:Play()]]--
	end)
	
	OpenClose.MouseButton1Click:Connect(function()
		if Executor.Position == UDim2.new(0.5, 0, 1.5, 0) then
			
			Executor:TweenPosition(UDim2.new(0.5, 0, 0.5, 0), "Out", "Quint", 0.7, true)
			
			OpenClose:TweenPosition(UDim2.new(0, 0,0.107, 0), "Out", "Quint", 0.7, true)
			OpenClose:TweenSize(UDim2.new(0, 80,0, 80), "Out", "Quint", 0.7, true)
			wait(0.7)
			Menu:TweenSize(UDim2.new(0, 80,0, 295), "Out", "Quint", 0.7, true)
			Menu:TweenPosition(UDim2.new(0, 0,0.608, 0), "Out", "Quint", 0.7, true)
			
	
			
	
	
			
		else
			Executor:TweenPosition(UDim2.new(0.5, 0, 1.5, 0), "Out", "Quint", 0.8, false)
			
	
			
			Menu:TweenSize(UDim2.new(0, 80,0, 0), "Out", "Quint", 0.4, true)
			Menu:TweenPosition(UDim2.new(0, 0,0.213, 0), "Out", "Quint", 0.4, true)
			wait(0.7)
			OpenClose:TweenPosition(UDim2.new(-0.006, 0,0.5, 0), "Out", "Quint", 0.8, true)
			OpenClose:TweenSize(UDim2.new(0, 38,0, 80), "Out", "Quint", 0.8, true)
	
			
	
			
		end
		
	
	
	end)
	
end
coroutine.wrap(TRSNFUZ_fake_script)()
