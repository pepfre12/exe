local function HTBI_fake_script() -- RJ.LocalScript 
	local script = Instance.new('LocalScript', RJ)

	RJ.MouseButton1Click:Connect(function()
		game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
	end)
end
coroutine.wrap(HTBI_fake_script)()
local function CZQN_fake_script() -- RS.LocalScript 
	local script = Instance.new('LocalScript', RS)

	RS.MouseButton1Click:Connect(function()
		HAXEXEV4:Destroy()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/pepfre12/exe/hax"))()
	end)
end
coroutine.wrap(CZQN_fake_script)()
local function AVCNDBB_fake_script() -- RE.LocalScript 
	local script = Instance.new('LocalScript', RE)

	RE.MouseButton1Click:Connect(function()
		
		game.Players.LocalPlayer.Character.Humanoid:TakeDamage(100000)
		
	end)
end
coroutine.wrap(AVCNDBB_fake_script)()
local function GBVG_fake_script() -- Executer.Dragify 
	local script = Instance.new('LocalScript', Executer)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
	    dragToggle = nil
	    local dragSpeed = 0
	    dragInput = nil
	    dragStart = nil
	    local dragPos = nil
	    function updateInput(input)
	        local Delta = input.Position - dragStart
	        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.25), {Position = Position}):Play()
	    end
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
	    end)
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
	    end)
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(GBVG_fake_script)()
