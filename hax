-- Scripts

local function WBZIOVJ_fake_script() -- Inject.LocalScript 
	local script = Instance.new('LocalScript', Inject)

	Inject.MouseButton1Click:Connect(function()
		
		--Inject stuff here
		
	end)
end
coroutine.wrap(WBZIOVJ_fake_script)()
local function KLKOGTU_fake_script() -- Clear.LocalScript 
	local script = Instance.new('LocalScript', Clear)

	Clear.MouseButton1Click:Connect(function()
		TextBox.Text = ""
	end)
end
coroutine.wrap(KLKOGTU_fake_script)()
